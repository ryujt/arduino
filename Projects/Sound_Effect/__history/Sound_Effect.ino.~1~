#include "musical_notes.h"

void setup() {
  pinMode(0, OUTPUT);
}

void loop() {
      seR2D2(0);
      delay(1000);
      
      seCloseEncounters(0);
      delay(1000);
      
      seAriel(0);
      delay(1000);

      seLaugh(0);
      delay(1000);
      
      seLaugh2(0);
      delay(1000);
      
      seSqueak(0);
      delay(1000);
      
      seWaka(0);
      delay(1000);  
      
      seCatcall(0);
      delay(1000);
      
      seOhhh(0);
      delay(1000);
      
      seUhoh(0);
      delay(1000);
      
  seChang(0);
  delay(1000);
  
  seCoo(0);
  delay(1000);
  
  seOh(0);
  delay(1000);  
  
  sePhone(0);
  delay(1000);    
  
  seSiren(0);
  delay(1000);    
  
  seSpace1(0);
  delay(1000);    
  
  seSpace2(0);
  delay(1000);    

  seFanfare(0);
  delay(1000);
  
  seLarm(0);
  delay(1000);
  
  seTelefone(0);
  delay(1000);  
  
  seTonarkade(0);
  delay(1000);    
  
  seTonarkade2(0);
  delay(1000);    
  
  seSad(0);
  delay(1000);
  
  seVictory(0);
  delay(1000);
  
  seAssorted(0);
  delay(1000);    
}
